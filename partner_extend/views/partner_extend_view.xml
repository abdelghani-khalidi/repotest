<odoo>
    <record id="view_excel_import_wizard_form" model="ir.ui.view">
        <field name="name">excel.import.wizard.form</field>
        <field name="model">excel.import.wizard</field>
        <field name="arch" type="xml">
            <form string="Importation depuis Excel">
                <group>
                    <field name="file" widget="binary" filename="filename" string="Fichier Excel" options="{'no_create': True, 'no_open': True}" />
                </group>
                <footer>
                    <button string="Importer" type="object" name="import_excel_data" class="btn-primary"/>
                    <button string="Annuler" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="action_excel_import_wizard" model="ir.actions.act_window">
        <field name="name">Importer depuis Excel</field>
        <field name="res_model">excel.import.wizard</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="view_excel_import_wizard_form"/>
        <field name="target">new</field>
    </record>

    

        <menuitem id="menu_excel_import" name="Importation Excel" parent="base.menu_warehouse_management" action="action_excel_import_wizard"/>
</odoo>
